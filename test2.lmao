var table = [[0, 0, 0],[0, 0, 0],[0, 0, 0]]

func printTable()
{
    outl("-----")
    var x = 0
    while (x < 3)
    {
        var y = 0
        while (y < 3)
        {
          out(table[x][y])
          y = y + 1
        }
        outl("")
        x = x + 1
    }
}

